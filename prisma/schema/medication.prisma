enum MedicationRoute {
  ORAL
  Nebulizer
  Intravenous
  Intramuscular
  INHALED
  TOPICAL
}

model Medication {
  id String @id @default(uuid())

  name         String
  standardDose String           @db.VarChar(100)
  route        MedicationRoute?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  listOfMadications ListOfMadications[]

  @@map("medications")
}
