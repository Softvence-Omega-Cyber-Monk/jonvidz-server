model VentSetting {
  id String @id @default(uuid())

  tidalVolumeOrPressure Decimal? @db.Decimal(10, 2)
  iTimeSec              Decimal? @db.Decimal(5, 2)
  ieRatio               Decimal? @db.Decimal(4, 2)
  riseTime              Decimal? @db.Decimal(5, 2)
  iTrigger              Decimal? @db.Decimal(4, 2)
  fiO2Pct               Decimal? @db.Decimal(5, 2)
  heatedHumidifierTemp  Decimal? @db.Decimal(5, 2)

  flowSheetId String    @unique
  flowSheet   FlowSheet @relation(fields: [flowSheetId], references: [id], onDelete: Cascade)

  patientId String
  patient   Patient @relation(fields: [patientId], references: [id], onDelete: Cascade)

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("vent_settings")
}
